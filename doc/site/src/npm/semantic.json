        "reset",
        "site",
        "button",
        "container",
        "divider",
        "header",
        "icon",
        "image",
        "label",
        "list",
        "rail",
        "segment",
        "grid",
        "menu",
        "message",
        "card",
        "item",
        "statistic",
        "sidebar",
        "transition"
